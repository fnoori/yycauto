{
  "name": "yycauto",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "dev-server": "NODE_ENV=development nodemon src/server/main.js",
    "dev-client": "NODE_ENV=development vue-cli-service serve",
    "dev": "concurrently \"npm run dev-client\" \"npm run dev-server\"",
    "dev-server-aws": "NODE_ENV=development-aws nodemon src/server/main.js",
    "dev-client-aws": "NODE_ENV=development-aws nodemon vue-cli-service serve",
    "dev-aws": "concurrently \"npm run dev-server-aws\" \"npm run dev-client-aws\""
  },
  "dependencies": {
    "argon2": "^0.21.0",
    "aws-sdk": "^2.429.0",
    "body-parser": "^1.18.3",
    "buefy": "^0.7.3",
    "concurrently": "^4.1.0",
    "dot-object": "^1.7.1",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "express-validator": "^5.3.1",
    "fs": "0.0.1-security",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.4.20",
    "multer": "^1.4.1",
    "multer-s3": "^2.9.0",
    "node-sass": "^4.11.0",
    "nodemon": "^1.18.10",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "quasar": "^1.0.0-beta.13",
    "sass-loader": "^7.1.0",
    "underscore": "^1.9.1",
    "validator": "^10.11.0",
    "vue": "^2.6.6",
    "vue-router": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.5.0",
    "@vue/cli-plugin-eslint": "^3.5.0",
    "@vue/cli-service": "^3.5.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "vue-template-compiler": "^2.5.21"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/standard"
    ],
    "rules": {
      "camelcase": "off",
      "no-useless-escape": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
